<!-- btdata/robot_bt.xml -->
<root main_tree_to_execute="NavigationTree">
  <BehaviorTree ID="NavigationTree">
    <Sequence name="root_sequence">
      <RobotStateChecker name="check_robot_state"/>
      <RecoveryNode number_of_retries="6" name="navigate_with_replanning">
        <PipelineSequence name="navigation_pipeline">
          <ComputePathToPose goal="{goal}"/>
          <FollowPath path="{path}"/>
        </PipelineSequence>
        <ClearEntireCostmap service_name="global_costmap/clear_entirely_global_costmap"/>
      </RecoveryNode>
    </Sequence>
  </BehaviorTree>
</root>